								"exec": [
									"let query = pm.response.json().data",
									"let course_index_with_lessons = query.findIndex((value) => {",
									"    if (value.lessons.length > 0){",
									"        return value;",
									"    }",
									"})",
									"",
									"pm.environment.set(\"last_created_course_id\", query[query.length-1].id);",
									"pm.environment.set('course_with_lessons', course_index_with_lessons + 1)",
									"pm.environment.set('lesson_course_id', query[course_index_with_lessons].lessons[0].id)"
